#ifndef HAL_LCD_LCD_CONFIG_H_
#define HAL_LCD_LCD_CONFIG_H_

#define LCD_PORT  PA
#define RS        PINA1
#define EN        PINA2

#define _8_BIT    1
#define _4_BIT    0

#define LCD_MODE  _4_BIT


#define D4  PINA3
#define D5  PINA4
#define D6  PINA5
#define D7  PINA6



#endif /* HAL_LCD_LCD_CONFIG_H_ */
