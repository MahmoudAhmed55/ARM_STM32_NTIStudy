#ifndef MCAL_LCD_LCD_CONFIG_H_
#define MCAL_LCD_LCD_CONFIG_H_

#define LCD_PORT  PA
#define RS        PINB0
#define EN        PINB1

#define _8_BIT    1
#define _4_BIT    0

#define LCD_MODE  _8_BIT

#define D4  PINA5
#define D5  PINA6
#define D6  PINA7
#define D7  PINA8



#endif /* MCAL_LCD_LCD_CONFIG_H_ */
