#ifndef MCAL_SPI_SPI_PRIVATE_H_
#define MCAL_SPI_SPI_PRIVATE_H_

#define   SPI_BASEADDRESS      (0x40013000)

#define SPI_CR1                      *((volatile u32*) (SPI_BASEADDRESS +0x00))

#define MSTR   3

#define SPI_CR2                      *((volatile u32*) (SPI_BASEADDRESS +0x04))
#define SPI_SR                       *((volatile u32*) (SPI_BASEADDRESS +0x08))
#define SPI_DR                       *((volatile u32*) (SPI_BASEADDRESS +0x0C))


#define SPI_CRCPR                    *((volatile u32*) (SPI_BASEADDRESS +0x10))
#define SPI_RXCRCR                   *((volatile u32*) (SPI_BASEADDRESS +0x14))
#define SPI_TXCRCR                   *((volatile u32*) (SPI_BASEADDRESS +0x0C))
#define SPI_I2SCFGR                  *((volatile u32*) (SPI_BASEADDRESS +0x10))
#define SPI_I2SPR                    *((volatile u32*) (SPI_BASEADDRESS +0x14))


#define SLAVE 0
#define MASTER 1


#define fPCLK/2    000
#define fPCLK/4    001
#define fPCLK/8    010
#define fPCLK/16   011
#define fPCLK/32   100
#define fPCLK/64   101
#define fPCLK/128  110
#define fPCLK/2    111


#endif /* MCAL_SPI_SPI_PRIVATE_H_ */
