#ifndef MCAL_LCD_LCD_CONFIG_H_
#define MCAL_LCD_LCD_CONFIG_H_

#define LCD_PORT  PA
#define RS        PINB0
#define EN        PINB1

#define _8_BIT    1
#define _4_BIT    0

#define LCD_MODE  _4_BIT

#define D4  PINA2
#define D5  PINA3
#define D6  PINA4
#define D7  PINA5



#endif /* MCAL_LCD_LCD_CONFIG_H_ */
