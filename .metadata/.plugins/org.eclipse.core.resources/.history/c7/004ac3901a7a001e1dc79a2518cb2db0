#ifndef _STD_TYPES_H_
#define _STD_TYPES_H_



typedef char                c8;
typedef signed char         s8;
typedef unsigned char       u8;

typedef short               s16;
typedef signed short        ss16;
typedef unsigned short      u16;

typedef int                 i32;
typedef unsigned long int   u32;
typedef signed long int     s32;

typedef float               f32;
typedef double              f64;




#endif
