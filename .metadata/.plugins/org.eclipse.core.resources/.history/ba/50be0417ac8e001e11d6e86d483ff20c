#ifndef MCAL_UART_UART_PRIVATE_H_
#define MCAL_UART_UART_PRIVATE_H_


#define SYSTICK_BASEADDRESS                  0xE000E010

#define USART_SR                         (*((volatile u32*)(SYSTICK_BASEADDRESS +0x00)))
#define USART_DR                         (*((volatile u32*)(SYSTICK_BASEADDRESS +0x00)))
#define USART_BRR                         (*((volatile u32*)(SYSTICK_BASEADDRESS +0x00)))
#define USART_CR1                         (*((volatile u32*)(SYSTICK_BASEADDRESS +0x00)))
#define USART_CR2                         (*((volatile u32*)(SYSTICK_BASEADDRESS +0x00)))


#endif /* MCAL_UART_UART_PRIVATE_H_ */
